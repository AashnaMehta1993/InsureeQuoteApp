@model IEnumerable<InsureeQuoteApp.Models.Insuree>

<h2>All Insurees</h2>

<table border="1" cellpadding="5" cellspacing="0">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Quote ($)</th>
    </tr>

@foreach (var i in Model)
{
    <tr>
        <td>@i.FirstName</td>
        <td>@i.LastName</td>
        <td>@i.EmailAddress</td>
        <td>@i.Quote</td>
    </tr>
}
</table>

<p>
    <a href="@Url.Action("Create", "Insuree")">Add New Insuree</a> |
    <a href="@Url.Action("Admin", "Insuree")">Admin View</a>
</p>
